{
  "name": "tweek-editor",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "Soluto/tweek",
  "author": "Soluto",
  "license": "MIT",
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "start": "yarn build-less && concurrently --kill-others \"yarn watch-less\" \"REACT_APP_GATEWAY_URL=http://localhost:4099 react-app-rewired start\"",
    "start:full-env": "yarn docker-compose up -d gateway publishing && yarn start",
    "build": "yarn build-less && react-app-rewired build",
    "test": "jest",
    "eject": "react-scripts eject",
    "teardown": "yarn docker-compose down --remove-orphans",
    "logs": "yarn docker-compose logs",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml -f ../../deployments/dev/docker-compose.local-editor.yml",
    "build-less": "node-less-chokidar src/",
    "watch-less": "yarn build-less && node-less-chokidar src/ -o src/ --watch --recursive"
  },
  "jest": {
    "testRegex": "/spec/unit/.*\\.js$",
    "coverageDirectory": "./coverage-unit/",
    "collectCoverage": true
  },
  "dependencies": {
    "adal-angular": "1.0.17",
    "animakit-expander": "^2.1.4",
    "chance": "^1.0.13",
    "change-case": "^3.0.2",
    "classnames": "^2.2.5",
    "emotion": "9.1.0",
    "express": "4.16.3",
    "fast-json-patch": "^2.0.6",
    "history": "^4.7.2",
    "idb-keyval": "^2.5.1",
    "jwt-decode": "2.2.0",
    "lunr": "^2.1.6",
    "moment": "^2.22.0",
    "oidc-client": "1.5.4",
    "papp-polyfill": "^1.1.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-oauth2": "^1.4.0",
    "prop-types": "^15.6.1",
    "query-string": "5.1.1",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-autosize-textarea": "^1.0.2",
    "react-calendar": "2.17.4",
    "react-document-title": "2.0.3",
    "react-dom": "^16.3.0",
    "react-draggable": "^3.0.5",
    "react-emotion": "9.1.0",
