    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-sanfona": "^1.1.0",
    "react-tabs": "^2.2.1",
    "react-tag-input": "^4.9.1",
    "react-tap-event-plugin": "^3.0.2",
    "react-tooltip": "^3.4.0",
    "react-virtualized": "^9.18.5",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
