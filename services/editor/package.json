    "papp-polyfill": "^1.1.0",
    "prop-types": "^15.6.1",
    "query-string": "^7.0.0",
    "ramda": "^0.27.1",
    "react": "^16.8.1",
    "react-autosize-textarea": "^7.1.0",
    "react-calendar": "^3.1.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^3.0.2",
    "react-document-title": "^2.0.3",
