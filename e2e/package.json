{
  "name": "e2e-tests",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "start": "yarn run docker-compose up -- --force-recreate -d editor",
    "test": "wdio wdio.conf.js",
    "rebuild": "yarn run docker-compose build",
    "test:full-env": "yarn run start && yarn run test",
    "test:full-env:rebuild": "yarn run rebuild && yarn run test:full-env",
    "test:docker": "yarn run start && yarn run rebuild backoffice-e2e && yarn run docker-compose run -- --rm backoffice-e2e",
    "test:docker:rebuild": "yarn run rebuild && yarn run test:docker",
    "teardown": "yarn run docker-compose down -- --remove-orphans",
    "test-authoring-api": "mocha spec/authoring-api/*test.js --recursive --timeout 60000",
    "docker-test-authoring-api": "npm run docker-compose build authoring && npm run docker-compose up -- --force-recreate -d authoring api && sleep 5 && npm run test-authoring-api",
    "docker-compose": "docker-compose -f ../deployments/dev/docker-compose.yml -f ../deployments/dev/docker-compose.override.yml -f ../deployments/dev/docker-compose.e2e.yml"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "6.24.1",
    "babel-core": "^6.18.2",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bluebird": "^3.5.0",
    "chai": "4.1.0",
    "chai-string": "^1.4.0",
    "chance": "1.0.10",
    "jsonwebtoken": "^7.4.0",
    "moment": "2.18.1",
    "nconf": "^0.8.4",
    "ramda": "^0.24.0",
    "wdio-mocha-framework": "0.5.10",
    "wdio-spec-reporter": "0.1.0",
    "webdriverio": "4.8.0"
  },
  "devDependencies": {
    "mocha": "^3.4.2",
    "supertest": "^3.0.0",
    "wait-promise": "^0.4.1",
    "wdio-selenium-standalone-service": "0.0.9"
  }
}
